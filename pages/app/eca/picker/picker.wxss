@font-face {
    font-family: 'youyuan';
    src: url('https://whusu.oss-cn-shanghai.aliyuncs.com/dreamer/fonts/yueyuan.otf');
    font-weight: normal;
    font-style: normal;
}
page{
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    box-sizing: border-box;
    padding-left: 10rpx;
    padding-right: 10rpx
}
view.question{
    width: 100%;
    padding-left: 20rpx;
    padding-right: 20rpx;
    color: #0fd189;
    word-break: break-all;
    font-family: 'youyuan';
    margin-top: 20rpx;
    font-size: 45rpx;
    font-weight: 600;
    text-align: left
}

.btn{
    background-color: #67c6c2;
    color: white;
    width: 280rpx;
    height: 70rpx;
    border: none;
    border-radius: 50rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    margin-top: 100rpx
}

.back{
    position: fixed;
    top: 0;left: 0;right: 0;bottom: 0;
    z-index: -999;
    width: 100%;
    height: 100%;
}
picker{
    font-family: 'youyuan';
}