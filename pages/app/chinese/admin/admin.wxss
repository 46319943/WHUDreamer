page{
    background-color: #666666
}
.page{
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    width: 100%;
    padding-top: 50rpx;
    padding-left: 20rpx;
    padding-right: 20rpx;
    padding-bottom: 150rpx;
    
}
.navibox{
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 100rpx;
    background-color: white;
    box-shadow: 0rpx 1rpx 20rpx #777777;
    z-index: 999;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-self: center
}
.navi{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center
}
.navi>image{
    width: 30rpx;
    height: 30rpx;
}

.tabbar{
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    height: 50rpx;
    z-index: 999;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-self: center;
    background-color: #666666;
    box-shadow: 0rpx -1rpx 20rpx black;
    color: white
}

.box{
    width: 90%;
    height: auto;
    border-radius: 10rpx;
    box-shadow: 1rpx 1rpx 5rpx #777777;
    background-color: white;
    margin-top: 10rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.box>view{
    width: 100%;
    height: auto;
    margin-top: 3rpx;
    overflow: hidden;
    text-align: left;
    padding-left: 20rpx
}