page {
    padding-bottom: 130rpx
  }
  .warpper{
      display: flex;
      flex-direction: column;
      align-items: center;
  
  }
  
  .body {
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    width: 100%;
    height: 100%;
    position: absolute;top: 0;left: 0;right: 0;bottom: 0;
    background-color: transparent;
    padding-top: 0;
   }
  
  .box {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-content: center;
    padding-right: 50rpx;
    
  }
  
  .textbox{
    height: 170rpx;
    padding: 20rpx;
    background-color: white;
    width: 400rpx
  }
  .infoup {
    color: #777777;
    float: left;
    text-align: left
  }
  
  .infocenter {
    color: black;
    float: left;
    text-align: left;
    font-size: 30rpx;
    font-weight: 700
  }
  .infobottom {
    color: #777777;
    float: left;
    text-align: left;
  }
  
  image.code{
    height: 70rpx;
    width: 70rpx;
    margin-top: 40rpx;
    
  }

  .add {
    width: 110rpx;
    height: 110rpx;
    border-radius: 60rpx;
    background-color: rgb(36, 176, 252);
    display: flex;
    justify-content: center;
    align-items: center;
    position: fixed;
    bottom: 100rpx;
    right: 30rpx;
    box-shadow: 0 0 20rpx #D8D8D8;
    z-index: 999999999
  } 

  .plus {
    width: 70rpx;
    height: 70rpx;
    z-index: 999999999
  }

  picker{
    border-bottom: solid 2rpx #777777;
    margin-top: 20rpx
  }

  input{
    border-bottom: solid 2rpx #777777;
    margin-top: 20rpx
  }

.btn{
  margin-top: 10rpx
}
    